#include <iostream>
using namespace std;
int main(){
    double n1, n2, promedio;
    cout<<"Ingrese numero 1: ";
    cin>>n1;
    cout<<"Ingrese numero 2: ";
    cin>>n2;
    promedio=n1+n2;
    cout<<"Ingrese numero 3: ";
    cin>>n1;
    cout<<"Ingrese numero 4: ";
    cin>>n2;   
    promedio=promedio+n1+n2;
    cout<<"Ingrese numero 5: ";
    cin>>n1;
    promedio=promedio+n1;
    cout<<"El promedio es: "<<promedio/5;
    return 0;
}
