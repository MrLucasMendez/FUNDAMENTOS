#include <iostream>
using namespace std;
int main()
{
    int n, acum = 1, i;
    do {
        cout << "Ingrese el numero de elementos: "; cin >> n;
    } while (n < 0);
    for (i = 2; i <= n; i++) {
        acum = acum * i;
    }
    cout << "El factorial de " << n << " es :" << acum;
    return 0;
    }
